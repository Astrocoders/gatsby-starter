{
  "name": "gatsby-starter",
  "version": "1.0.0",
  "author": "Astrocoders <hello@astrocoders.com>",
  "dependencies": {
    "gatsby": "^2.0.0",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-manifest": "^1.0.27",
    "gatsby-plugin-nprogress": "^1.0.14",
    "gatsby-plugin-offline": "^1.0.18",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-styled-components": "^3.0.0",
    "jest-dom": "^2.0.5",
    "jest-fetch-mock": "^1.6.6",
    "jest-localstorage-mock": "^2.2.0",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.4.9"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "start": "npm run clean && npm run dev",
    "build": "gatsby build",
    "clean": "rm -rf .cache && rm -rf public",
    "dev": "gatsby develop --open",
    "lint:staged": "lint-staged",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write --single-quote true --trailing-comma all --print-width 120 --semi false",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(\\.(test|spec))\\.(jsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "\\.(scss|svg|png|jpg)$": "<rootDir>/src/styleMock.js"
    },
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "setupTestFrameworkScriptFile": "./src/setupTests.js",
    "testURL": "http://localhost"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.1",
    "lint-staged": "^7.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.0",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-testing-library": "^5.1.0"
  }
}
